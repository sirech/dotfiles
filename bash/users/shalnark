# -*- mode: Shell-script-*-

#
# Settings for the machine 'shalnark'
#

## ENV

export _DOCS="${HOME}/Documents"
export _WORKSPACE="${_DOCS}/workspace"

## ALIASES

# common dirs
alias gostuff="cd ${_WORKSPACE}/stuff"
alias goalgs="cd ${_WORKSPACE}/algorithms"
alias gores="cd ${_DOCS}/resume"
alias gocovers="cd ${_DOCS}/resume/covers-$(year)"
